{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
    "max-len": ["error", { "code": 120 }],
    "require-jsdoc": "off",
        "valid-jsdoc": "off",
        "eol-last": "off"
    }
}
